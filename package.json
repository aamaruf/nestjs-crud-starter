{
  "name": "nestjs-crud-starter",
  "version": "0.0.1",
  "description": "",
  "author": "Zahid Hasan<zahidhasan065@gmail.com>",
  "private": false,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:migration:create": "yarn typeorm migration:create",
    "db:migration:run": "yarn typeorm migration:run -d dist/database/ormconfig.js",
    "db:migration:revert": "yarn typeorm migration:revert -d dist/database/ormconfig.js",
    "db:drop": "yarn typeorm schema:drop -d dist/database/ormconfig.js"
  },
  "dependencies": {
    "@anchan828/nest-bullmq": "^3.1.18",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.10",
    "@nestjs/core": "^11.0.10",
    "@nestjs/cqrs": "^11.0.2",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.0.5",
    "@nestjs/typeorm": "^11.0.0",
    "aws-sdk": "^2.1320.0",
    "axios": "^1.3.2",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.1",
    "bullmq": "^5.41.5",
    "cache-manager": "^6.4.0",
    "cache-manager-redis-store": "3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "express-rate-limit": "^7.5.0",
    "googleapis": "^144.0.0",
    "handlebars": "^4.7.7",
    "helmet": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.8.0",
    "nodemailer": "^6.9.1",
    "otp-without-db": "^1.0.4",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.0",
    "slugify": "^1.6.5",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.11",
    "util": "^0.12.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.4",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.10",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^5.0.0",
    "@types/csurf": "^1.11.2",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^6.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "^22.13.5",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "29.7.0",
    "prettier": "^3.5.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.6",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
